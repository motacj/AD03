<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
    <class name="model.Profesor" table="profesor">
        <id name="idProf" column="idProf" type="integer">
            <generator class="identity"/>
        </id>
        <property name="nombre" column="nombre" type="string"/>
        <property name="apellidos" column="apellidos" type="string"/>
        <property name="instituto" column="instituto" type="string"/>
        <property name="asignatura" column="asignatura" type="string"/>
    </class>
</hibernate-mapping>